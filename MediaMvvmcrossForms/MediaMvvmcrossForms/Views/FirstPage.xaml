<?xml version="1.0" encoding="utf-8" ?>
<core:MvxContentPage xmlns="http://xamarin.com/schemas/2014/forms"
                     xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                     x:Class="MediaMvvmcrossForms.Views.FirstPage"
                     xmlns:core="clr-namespace:MvvmCross.Forms.Core;assembly=MvvmCross.Forms">
  <StackLayout>
    <Label Text="First" />
    <Slider x:Name="PlaybackSlider"
            ValueChanged="Slider_OnValueChanged" />
    <Button x:Name="PlayButton"
            Command="{Binding PlayCommand}"
            Text="Play" />
    <Button x:Name="PauseButton"
            Command="{Binding PauseCommand}"
            Text="Pause" />
    <Button Command="{Binding NavigateCommand}"
            Text="Go to Second Page" />
  </StackLayout>
</core:MvxContentPage>